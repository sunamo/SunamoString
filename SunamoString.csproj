<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Version>24.11.9.1</Version>
    <Description>A large number of methods for string operations</Description>
    <Authors>www.sunamo.cz</Authors>
    <Copyright>www.sunamo.cz</Copyright>
    <PackageProjectUrl>https://sunamo.cz/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/sunamo/SunamoString.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReadmeFile>readme.md</PackageReadmeFile>
    <PackageReleaseNotes>fix when is no delimiter</PackageReleaseNotes>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="_sunamo\SunamoEnums\**" />
    <Compile Remove="_sunamo\SunamoStringShared\**" />
    <EmbeddedResource Remove="_sunamo\SunamoEnums\**" />
    <EmbeddedResource Remove="_sunamo\SunamoStringShared\**" />
    <None Remove="_sunamo\SunamoEnums\**" />
    <None Remove="_sunamo\SunamoStringShared\**" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Diacritics" Version="3.3.27" />
    <None Include="readme.md" Pack="True" PackagePath="." />
  </ItemGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>$(DefineConstants);ASYNC;SunamoString;SunamoString.csproj</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>$(DefineConstants);ASYNC;SunamoString;SunamoString.csproj</DefineConstants>
  </PropertyGroup>
</Project>